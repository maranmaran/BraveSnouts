    "@angular/forms": "~16.1.6",
    "@angular/material": "^16.1.5",
    "@angular/material-date-fns-adapter": "^16.1.5",
    "@angular/platform-browser": "~16.1.6",
    "@angular/platform-browser-dynamic": "~16.1.6",
    "@angular/router": "~16.1.6",
    "@contentful/rich-text-html-renderer": "^16.1.1",
    "@ngneat/hot-toast": "^5.0.2",
    "@ngneat/overview": "^5.1.1",
    "@stripe/stripe-js": "^1.54.2",
    "@tweenjs/tween.js": "^21.0.0",
