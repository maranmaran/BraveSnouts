{
  "name": "brave-snouts-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./src/environments/set-env.script.ts",
    "start": "npm run config -- --environment=dev && ng serve -c development",
    "start-ssl": "npm run config -- --environment=dev && ng serve -c development --ssl --ssl-key ./src/environments/https/localhost.key --ssl-cert ./src/environments/https/localhost.crt",
    "build-dev": "npm run config -- --environment=dev && ng build -c development",
    "build-prod": "npm run config -- --environment=prod && ng build -c production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.1.1",
    "@angular/cdk": "^14.1.1",
    "@angular/common": "~14.1.1",
    "@angular/compiler": "~14.1.1",
    "@angular/core": "^14.1.1",
    "@angular/fire": "^7.4.1",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "~14.1.1",
    "@angular/material": "^14.1.1",
    "@angular/material-moment-adapter": "^14.1.1",
    "@angular/platform-browser": "~14.1.1",
    "@angular/platform-browser-dynamic": "~14.1.1",
    "@angular/router": "~14.1.1",
    "@ngneat/hot-toast": "^4.1.0",
    "@ngneat/overview": "^3.0.4",
    "@tweenjs/tween.js": "^18.6.4",
    "bootstrap": "^5.2.0",
    "firebase": "^9.9.2",
